{
	"name": "typed-numbers",
	"description": "Super simple type-safe fixed-length integers for TypeScript/JavaScript",
	"version": "1.0.3",
	"author": "Skayo (https://skayo.dev)",
	"license": "MIT",
	"repository": "Skayo/TypedNumbers",
	"homepage": "https://github.com/Skayo/TypedNumbers",
	"bugs": "https://github.com/Skayo/TypedNumbers/issues",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"module": "dist/typed-numbers.esm.js",
	"unpkg": "dist/typed-numbers.umd.production.min.js",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsdx build --format cjs,umd,esm --name TypedNumbers",
		"dev": "tsdx watch",
		"prepare": "npm run build"
	},
	"engines": {
		"node": ">=10.0.0"
	},
	"keywords": [
		"int8",
		"uint8",
		"int16",
		"uint16",
		"int32",
		"uint32",
		"int64",
		"uint64",
		"int128",
		"uint128",
		"typed-array",
		"alternative",
		"implementation",
		"native"
	],
	"devDependencies": {
		"microbundle": "^0.13.3",
		"tsdx": "^0.14.1"
	}
}
